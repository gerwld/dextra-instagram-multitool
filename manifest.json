{
    "manifest_version": 3,
    "name": "Chrome extension",
    "version": "1.0.0",
    "action": { "default_popup": "index.html" },
    "icons": {
    },
    "permissions": [
        "activeTab",
        "scripting"
      ],
      "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ]
  }